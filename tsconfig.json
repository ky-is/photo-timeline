{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"sourceMap": true,
		"resolveJsonModule": false,
		"esModuleInterop": true,
		"lib": ["esnext", "dom"],
		"types": ["vite/client", "node"],
		"allowJs": true,
		"checkJs": true,
	},
	"include": [
		"./site/.vitepress/**/*.ts",
		"./site/.vitepress/**/*.vue",
	],
}
